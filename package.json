    "upDep": "./node_modules/.bin/npm-check-updates -ua"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poppinlp/js-api-generator.git"
  },
  "keywords": [
    "js",
    "api",
    "generator",
    "request",
    "fetch"
  ],
  "author": "PoppinL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/poppinlp/js-api-generator/issues"
  },
  "homepage": "https://github.com/poppinlp/js-api-generator#readme",
  "dependencies": {
    "browserify": "^14.1.0",
    "hogan.js": "^3.0.2",
    "js-yaml": "^3.8.2",
    "lodash": "^4.17.4",
    "mkpath": "^1.0.0",
    "rollup": "^0.41.6"
  },
  "devDependencies": {
    "npm-check-updates": "^2.10.4",
    "xo": "^0.18.1"
  },
  "xo": {
	  "rules": {
		  "no-unused-expressions": [
